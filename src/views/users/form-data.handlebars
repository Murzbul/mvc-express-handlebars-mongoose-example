 <div class="a">
    <div class="row">
        <h3>Register Today</h3>
        <p>Fill in the data below.</p>
        <form action="/data-onwire" method="POST" class="requires-validation" novalidate>

            <div class="mb-3 col-md-12">
               <input class="form-control" type="text" name="firstName" placeholder="First Name" required>
            </div>

            <div class="mb-3 col-md-12">
                <input class="form-control" type="text" name="lastName" placeholder="Last Name" required>
            </div>

            <div class="mb-3 col-md-12">
                <input class="form-control" type="text" name="dni" placeholder="Dni" required>
            </div>

            <div class="form-button mt-3">
                <button id="submit" type="submit" class="btn btn-primary">Register</button>
            </div>
        </form>
    </div>
</div>
 <div class="align-content-center">
     <h3>Users List</h3>
     <table class="table table-dark">
      <thead>
        <tr>
          <th scope="col">#ID</th>
          <th scope="col">FirstName</th>
          <th scope="col">LastName</th>
          <th scope="col">Dni</th>
        </tr>
      </thead>
      <tbody id="user-table">
      </tbody>
</table>
     <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.2.3/axios.min.js"></script>
     <script>
          axios.get("http://localhost:8080/data-onwire/json").then((result) =>
          {
              const { users } = result.data;
              let htmlTable = '';

              users.forEach((user)=>{
                    htmlTable += `<tr class="table-light">
                       <th scope="row">${user._id}</th>
                       <td>${user.firstName}</td>
                       <td>${user.lastName}</td>
                       <td>${user.dni}</td>
                    </tr>`
              })

              let tmpObj = document.getElementById("user-table");
              tmpObj.innerHTML = htmlTable;
          });
     </script>
 </div>
